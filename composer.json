{
    "name": "magento/magento2ce",
    "description": "Magento 2 (Open Source)",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "~7.4.0||~8.1.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo_mysql": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-sodium": "*",
        "ext-xsl": "*",
        "ext-zip": "*",
        "lib-libxml": "*",
        "amasty/module-customer-attributes": "2.3.1",
        "amasty/module-shipping-suite-meta": "1.0.1",
        "amasty/module-store-locator": "2.5.1",
        "amasty/number": "1.1.7",
        "amasty/payrestriction": "2.3.11",
        "amasty/sorting": "2.8.12",
        "amasty/groupcat": "1.7.3",
        "amasty/adminactionslog": "1.2.13",
        "commersys/announcementbar": "1.1.0",
        "composer/composer": "^1.9",
        "lcobucci/jwt": "^3.2",
        "mage_delight/megamenu": "2.0.16",
        "mageme/module-webforms-3": "^3.0",
        "magento/composer-dependency-version-audit-plugin": "~0.1",
        "magento/composer-root-update-plugin": "~1.1",
        "magento/composer": "~1.5.0",
        "magento/magento-composer-installer": ">=0.1.11",
        "magento/data-migration-tool": "2.4.0",
        "magento/product-community-edition": "2.4.3",
        "olegkoval/magento2-regenerate-url-rewrites": "^1.6",
        "paradoxlabs/authnetcim": "4.3.8",
        "sparsh/magento-2-smtp-extension": "1.0.1"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "~1.5.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "friendsofphp/php-cs-fixer": "~3.4.0",
        "lusitanian/oauth": "~0.8.10",
        "magento/magento-coding-standard": "*",
        "pdepend/pdepend": "~2.10.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpmd/phpmd": "^2.12.0",
        "phpstan/phpstan": "^1.5.7",
        "phpunit/phpunit": "~9.5.20",
        "sebastian/phpcpd": "^6.0.3",
        "squizlabs/php_codesniffer": "~3.6.0",
        "symfony/finder": "^5.2"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "version": "2.4.3",
    "minimum-stability": "stable"
}
